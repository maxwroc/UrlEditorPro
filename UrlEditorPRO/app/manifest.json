{
  "manifest_version": 2,

  "name": "Url Editor PRO",
  "description": "Url parser which makes editing complex URLs as easy as possible. Features: params decoding, auto-suggest, keyboard navigation..",
  "version": "0.9.2.1",

  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "windows",
    "webRequest",
    "webRequestBlocking",
    "contextMenus"
  ],
  "icons": {
    "16": "img/edit_16.png",
    "48": "img/edit_32.png",
    "128": "img/edit.png"
  },
  "browser_action": {
    "default_icon": "img/edit_32.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [ "app.js", "background.js" ]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      }
    },
    "GoToHomepage": {
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "mac": "Command+Shift+H"
      },
      "description": "Go to homepage (root path)"
    },
    "RedirectUseFirstRule": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Redirect using first rule"
    }
  },
  "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'"
}